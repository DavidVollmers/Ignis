{
  "scripts": {
    "build:css": "npx tailwindcss -i ./Styles/tailwind.css -o ./wwwroot/css/tailwind.min.css --minify",
    "build:js": "npx webpack ./Scripts/website.js -o ./wwwroot/js --output-filename website.min.js",
    "build": "npm run build:css && npm run build:js"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "prismjs": "^1.29.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  }
}
