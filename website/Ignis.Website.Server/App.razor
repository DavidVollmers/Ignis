@using Ignis.Website.Pages
<Router AppAssembly="@typeof(App).Assembly" AdditionalAssemblies="new[] { typeof(PageLayout).Assembly }">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(PageLayout)"/>
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(CommonLayout)"/>
    </NotFound>
</Router>