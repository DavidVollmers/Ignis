@using Ignis.Website.Pages
@using Index = Ignis.Website.Index
<Router AppAssembly="@typeof(App).Assembly" AdditionalAssemblies="new[] { typeof(Index).Assembly }">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(PageLayout)"/>
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(Index)"/>
    </NotFound>
</Router>